    "import numpy as np\n",
    "\n",
    "z = np.random.randn(2)\n",
    "softmax = np.exp(z - z.max()) / np.exp(z - z.max()).sum()\n",
    "\n",
    "# argument is the difference between the two actions\n",
    "p1 = 1 / (1 + np.exp(-(z[0] - z[1])))\n",
    "\n",
    "print(softmax[0], p1) "
